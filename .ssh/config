#SSH config file ya'll.

Host chandra 
    HostName chandra.cs.pdx.edu
    User morndoff

Host rita 
    HostName rita.cat.pdx.edu
    User morndoff

Host ruby
    HostName ruby.cat.pdx.edu
    User morndoff

Host rorschach  
    HostName 174.138.76.111 
    User morndoff

# ssh-agent. For macOS Sierra 10.12.2 or later. 
# Load keys into ssh-agent. Store passphrases in keychain.
Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa
